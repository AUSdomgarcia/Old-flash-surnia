package com.surnia.socialStar.views.isoItems.miniProgressbar.events 
{
	import flash.events.Event;
	
	/**
	 * ...
	 * @author monsterpatties
	 */
	public class MiniProgressBarEvent extends Event 
	{
		/*----------------------------------------------------------------Constant------------------------------------------------------------------------*/
		public static const ON_PROGRESS_DONE:String = "ON_PROGRESS_DONE";	
		/*----------------------------------------------------------------Properties---------------------------------------------------------------------*/
		
		/*----------------------------------------------------------------Constructor--------------------------------------------------------------------*/
		
		
		public function MiniProgressBarEvent(type:String, bubbles:Boolean=false, cancelable:Boolean=false) 
		{ 
			super(type, bubbles, cancelable);
			
		} 
		
		/*----------------------------------------------------------------Methods------------------------------------------------------------------------*/
		
		public override function clone():Event 
		{ 
			return new MiniProgressBarEvent(type, bubbles, cancelable);
		} 
		
		public override function toString():String 
		{ 
			return formatToString("MiniProgressBarEvent", "type", "bubbles", "cancelable", "eventPhase"); 
		}
		
		/*----------------------------------------------------------------Setters------------------------------------------------------------------------*/
		
		/*----------------------------------------------------------------Getters------------------------------------------------------------------------*/
		
		/*----------------------------------------------------------------EventHandlers------------------------------------------------------------------*/	
		
	}
	
}