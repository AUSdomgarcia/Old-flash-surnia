package com.surnia.socialStar.utils.fonttextmanager
{
	import flash.text.Font;
	import flash.text.TextField;
	import flash.text.TextFormat;

	public class TextFieldUtility
	{
		public function TextFieldUtility()
		{
			
		}
		
		public static function initTextFontFormat(font:Font, textField:TextField, textFormat:TextFormat ):void{
			textFormat.font = font.fontName;
			
			textField.embedFonts = true;
			textField.defaultTextFormat = textFormat;
			// add additional paramters
		}
	}
}