package com.surnia.socialStar.ui.popUI.views.eventScene.event
{
	import flash.events.Event;

	public class EventSceneEvent extends Event
	{
		public var params:Object = new Object;
		
		public static const EVENTSCENE_CONTINUE:String = "onContinueClicked";
		public static const EVENTSCENE_DONE:String = "onDoneClicked";
		public static const EVENTSCENE_UPDATEPOSTION = "onUpdatePosition";
		
		public function EventSceneEvent(type:String, Params:Object = null, bubbles:Boolean = false, cancelable:Boolean = false)
		{
			params = Params;
			super(type, bubbles, cancelable);
		}
		
		
	}
}