	public class BuildingDetail 
	{		
		public var buildingURL:String;
		public var buildingDimURL:String;
		public var buildingBrightURL:String;
		
		public var buildingName:String;
		public var buildingMode:String;
		public var buildingID:String;
		
		public var posX:int;
		public var posY:int;
		public var posZ:int;		
		
		public var buildingLockURL:String;
		public var buildingIsBoss:String;
		
		public var levelDetail:LevelDetail;		
	
		public function BuildingDetail(iconURL:String = null,iconDimURL:String = null, iconBrightURL:String = null, X:int = 0, Y:int = 0, Z:int=0, areaName:String = null, iconID:String = null, modeType:String = null, isBoss:String = null, lvlDetail:LevelDetail = null, lockURL:String = null) 
		{			
			buildingID		= iconID;
			
			buildingURL			= iconURL;
			buildingDimURL		= iconDimURL;
			buildingBrightURL	= iconBrightURL;
			
			buildingName	= areaName;
			buildingMode	= modeType;
			
			buildingIsBoss	= isBoss;
			posX			= X;
			posY			= Y;
			posZ			= Z;	
			
			levelDetail	= lvlDetail;
			buildingLockURL	= lockURL;			
		}		
	}

}