package com.surnia.socialStar.ui.component.progressBarProxy.event
{
	import flash.events.Event;

	public class ProgressBarEventProxy extends Event
	{
		public var params:Object = new Object;
		
		public static const PROGRESS_COMPLETE:String = "inProgressComplete";
		public static const PROGRESSBAR_REMOVE:String = "onProgressBarRemove";
		
		public function ProgressBarEventProxy(type:String, Params:Object = null, bubbles:Boolean = false, cancelable:Boolean = false)
		{
			params = Params;
			super(type, bubbles, cancelable);
		}
	}
}