package com.surnia.socialStar.ui.contestantinformation.event
{
	import flash.events.Event;

	public class ContestantInformationEvent extends Event
	{
		public var params:Object = new Object;
		
		public static const CLOSE_CLICK:String = "onCloseClick";
		public static const OK_CLICK:String = "onOkClick";
		public static const SHOW_INFOWINDOW:String = "onShowInfoWindow";
		public static const HIDE_INFOWINDOW:String = "onHideInfoWindow";
		
		public function ContestantInformationEvent(type:String, Params:Object = null, bubbles:Boolean = false, cancelable:Boolean = false)
		{
			params = Params;
			super(type, bubbles, cancelable);
		}
	}
}